{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "target": "ES2020",
    "skipLibCheck": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "moduleResolution": "node16"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}